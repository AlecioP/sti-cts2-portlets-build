    <entry>
      <name>UPDATE primo livello tasssonomia</name>
      <description/>
      <type>SQL</type>
      <sql>UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;apparato gastrointestinale e metabolismo&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;A&#x27;&#x3b;&#xd;&#xa; &#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;sangue e sistema emopoietico&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;B&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;apparato cardiovascolare&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;C&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;apparato tegumentario e pelle&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;D&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;apparato genito-urinario e ormone sessuale&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;G&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;sistema endocrino, esclusi ormoni sessuali e insulina&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;H&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;anti-infettivi per uso sistemico&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;J&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;antineoplastici e immunomodulatori&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;L&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;sistema muscolare - sistema scheletrico e articolazioni&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;M&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;sistema nervoso&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;N&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;prodotti antiparassitari, insetticidi e repellenti&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;P&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;apparato respiratorio&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;R&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;organi di senso&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;S&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;UPDATE temp_atc SET pl_gruppoanatomicoprincipale &#x3d; &#x27;vari&#x27;&#xd;&#xa;WHERE primo_livello like &#x27;V&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
