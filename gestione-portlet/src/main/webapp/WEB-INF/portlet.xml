<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>sti-gestione</portlet-name>
		<display-name>STI Gestione</display-name>
		<portlet-class>it.linksmt.cts2.portlet.search.GestionePortlet</portlet-class>
<!-- 		<init-param> -->
<!-- 			<name>edit-template</name> -->
<!-- 			<value>/edit.jsp</value> -->
<!-- 		</init-param> -->
<!-- 		<init-param> -->
<!-- 			<name>help-template</name> -->
<!-- 			<value>/help.jsp</value> -->
<!-- 		</init-param> -->
		<init-param>
			<name>view-template</name>
			<value>/views/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
<!-- 			<portlet-mode>EDIT</portlet-mode> -->
<!-- 			<portlet-mode>HELP</portlet-mode> -->
		</supports>
		<portlet-info>
			<title>STI Gestione Portlet</title>
			<short-title>STI Gestione</short-title>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>cross-mapping-sti</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>
